Program [
    FuncDef {
        symbol_name = "calculateBMI",
        function_rtype = TypeFloat,
        function_parameters = [
            VarDecl "weight" TypeFloat,
            VarDecl "height" TypeFloat
        ],
        function_gtypes = [],
        function_body = [
            Return
                (ExpDiv
                    (ExpVariable (VarAccess "weight" VarAccessNothing))
                    (ExpMul
                        (ExpVariable (VarAccess "height" VarAccessNothing))
                        (ExpVariable (VarAccess "height" VarAccessNothing))))
        ],
        symbol_pos = SrcPos (7, 6)
    },

    FuncDef {
        symbol_name = "isAdult",
        function_rtype = TypeBool,
        function_parameters = [
            VarDecl "age" TypeInt
        ],
        function_gtypes = [],
        function_body = [
            Return
                (ExpGeq
                    (ExpVariable (VarAccess "age" VarAccessNothing))
                    (ExpLitInteger 18))
        ],
        symbol_pos = SrcPos (12, 6)
    },

    FuncDef {
        symbol_name = "main",
        function_rtype = TypeVoid,
        function_parameters = [],
        function_gtypes = [],
        function_body = [
            VarDef
                (VarDecl "bmi" TypeFloat)
                (ExpFCall "calculateBMI" [
                    ExpLitFloating 70.5,
                    ExpLitFloating 1.75
                ]),

            VarDef
                (VarDecl "adult" TypeBool)
                (ExpFCall "isAdult" [
                    ExpLitInteger 20
                ]),

            CmdExpression
                (ExpFCall "print" [
                    ExpVariable (VarAccess "bmi" VarAccessNothing)
                ]),

            CmdExpression
                (ExpFCall "print" [
                    ExpVariable (VarAccess "adult" VarAccessNothing)
                ]),

            If
                (ExpAnd
                    (ExpVariable (VarAccess "adult" VarAccessNothing))
                    (ExpGt
                        (ExpVariable (VarAccess "bmi" VarAccessNothing))
                        (ExpLitFloating 25.0)))
                [
                    CmdExpression
                        (ExpFCall "print" [
                            ExpLitString "adulto com sobrepeso"
                        ])
                ]
                [
                    CmdExpression
                        (ExpFCall "print" [
                            ExpLitString "condição normal"
                        ])
                ]
        ],
        symbol_pos = SrcPos (17, 6)
    }
]